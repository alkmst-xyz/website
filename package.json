{
  "name": "website",
  "version": "0.1.0",
  "description": "Joseph Sebastian's website",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf _site",
    "dev": "npm-run-all clean --parallel dev:*",
    "dev:postcss": "npx postcss src/css/*.css --dir _site/css --watch",
    "dev:eleventy": "npx @11ty/eleventy --pathprefix 'website' --serve",
    "build": "npm-run-all clean build:postcss build:eleventy",
    "build:postcss": "NODE_ENV=production npx postcss src/css/*.css --dir _site/css",
    "build:eleventy": "ELEVENTY_PRODUCTION=true npx @11ty/eleventy --pathprefix 'website'",
    "serve": "npx @11ty/eleventy --serve"
  },
  "keywords": [],
  "author": "Joseph Sebastian",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/josephsv96/website/issues"
  },
  "homepage": "https://github.com/josephsv96/website#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
    "@tailwindcss/typography": "^0.5.7",
    "autoprefixer": "^10.4.12",
    "cssnano": "^5.1.13",
    "eleventy-plugin-time-to-read": "^1.2.0",
    "html-minifier": "^4.0.0",
    "luxon": "^2.5.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.5",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-texmath": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.17",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.1.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.1.8"
  },
  "dependencies": {
    "katex": "^0.16.2"
  }
}
