{
  "name": "website",
  "version": "0.1.0",
  "description": "Joseph Sebastian's website",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf _site",
    "postcss": "npx postcss --use cssnano --dir _site/css/code-style/ src/css/code-style",
    "dev:tailwind": "npx tailwindcss -i src/css/tailwind.css -o _site/css/styles.css --watch",
    "dev:eleventy": "npx @11ty/eleventy --serve",
    "build:tailwind": "npx tailwindcss -i src/css/tailwind.css -o _site/css/styles.css --minify",
    "build:eleventy": "ELEVENTY_PRODUCTION=true npx @11ty/eleventy --serve",
    "dev": "npm-run-all clean --parallel postcss dev:*",
    "build": "NODE_ENV=production npm-run-all clean postcss build:tailwind build:eleventy"
  },
  "keywords": [],
  "author": "Joseph Sebastian",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/josephsv96/website/issues"
  },
  "homepage": "https://github.com/josephsv96/website#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-navigation": "^0.3.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.8",
    "eleventy-plugin-lazyimages": "^2.1.2",
    "html-minifier": "^4.0.0",
    "luxon": "^2.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.13",
    "postcss-cli": "^9.1.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.24"
  }
}
